{
  "name": "flownote",
  "version": "1.0.0-alpha.6",
  "description": "Language for flow-based programming",
  "main": "index.js",
  "keywords": [
    "flow-based",
    "graph",
    "nodes",
    "channel",
    "channels",
    "parallel",
    "paralell",
    "map",
    "mindmap",
    "functional",
    "flow",
    "etl",
    "pipeline",
    "unity",
    "piping",
    "pipes",
    "stream",
    "streams"
  ],
  "scripts": {
    "test": "ava",
    "test-watch": "ava --watch",
    "test-coverage": "nyc ava",
    "profiler": "./node_modules/.bin/clinic doctor --autocannon / --method GET -- node profiler.js",
    "profiler-explorer": "./node_modules/.bin/clinic bubbleprof --autocannon / --method GET -- node profiler.js",
    "profiler-performance": "./node_modules/.bin/deoptigate -- node --allow-natives-syntax profiler.js",
    "browser-build": "./node_modules/.bin/webpack --mode production --colors",
    "browser-test": "./node_modules/.bin/webpack-dev-server --mode development --hot --colors",
    "preversion": "npm test",
    "version": "npm run browser-build && git add -A && git commit -m 'New Release'",
    "publish-next": "npm publish --tag next --access public",
    "publish-latest": "npm publish --tag latest --access public",
    "compile": "node -r esm compiler.js"
  },
  "license": "MIT",
  "repository": "Emblem21-OpenSource/flownote",
  "author": "Patrick Ryan <patrick.ryan@emblem21.com",
  "engines": {
    "node": ">=11.12.0"
  },
  "dependencies": {
    "colors": "^1.2.0-rc0",
    "esm": "^3.2.10",
    "fast-safe-stringify": "^2.0.6",
    "flatted": "^2.0.0",
    "hyperid": "^1.4.1",
    "ohm-js": "^0.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.5.0",
    "ava": "^1.0.0-rc.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^2.0.1",
    "clinic": "^4.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.2",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "eslint_d": "^7.3.0",
    "fuzzur": "^0.1.2",
    "jshint": "^2.10.2",
    "nyc": "^13.1.0",
    "terser": "^3.17.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-md5-hash": "^0.0.6"
  }
}
